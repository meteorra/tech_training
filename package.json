    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.5.1"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "history": "^4.6.3",
    "install": "^0.10.1",
    "isomorphic-fetch": "^2.2.1",
    "npm": "^5.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-css-modules": "^4.5.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
