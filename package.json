{
  "name": "tech-training",
  "description": "Training latest technologies",
  "version": "0.0.1",
  "author": "diana baranova",
  "license": "MIT",
  "homepage": "https://github.com/meteorra/tech-training",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "npmName": "tech-training",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meteorra/tech-training.git"
  },
  "bugs": {
    "url": "https://github.com/meteorra/tech-training/issues"
  },
  "keywords": [
    "tech-training"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --progress --colors --config webpack/webpack.config.development.babel.js",
    "lint": "eslint ./src --fix",
    "build": "webpack --config webpack/webpack.config.development.babel.js",
    "serve": "webpack --config webpack/webpack.config.production.babel.js",
    "preversion": "npm run clean && npm run lint",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "start": "npm run build && npm run dev",
    "test": "jest --config jest.config.json",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "es5-shim": "^4.3.1",
    "eslint": "^4.3.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "3.0.0-rc.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
